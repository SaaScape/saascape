{
  "name": "saascape_server",
  "version": "1.0.0",
  "description": "All-in-one software as a service management dashboard",
  "main": "server.ts",
  "scripts": {
    "build": "tsc && cp -r public dist/server/public && cpx 'scripts/**/*.sh' dist/server/scripts && cpx 'configs/*' dist/server/configs ",
    "test": "echo \"Error: no test specified\" && exit 1",
    "startd": "nodemon -r tsconfig-paths/register -r dotenv/config server.ts launch_command=startd",
    "startd-bg": "nodemon -r tsconfig-paths/register -r dotenv/config background/server.ts launch_command=startd-bg",
    "start": "node dist/server/server.js launch_command=start",
    "start-bg": "node dist/server/background/server.js launch_command=start-bg"
  },
  "author": "Keir Davie",
  "license": "GPL-3.0",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.6",
    "@types/dockerode": "^3.3.23",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.202",
    "@types/module-alias": "^2.0.4",
    "@types/ping": "^0.4.4",
    "cpx": "^1.5.0",
    "dotenv": "^16.3.1",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "acme-client": "^5.3.0",
    "bcrypt": "^5.1.1",
    "bull": "^4.12.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cron": "^3.1.6",
    "dockerode": "^4.0.2",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.3",
    "moment": "^2.29.4",
    "mongodb": "^6.3.0",
    "node-ssh": "^13.1.0",
    "nodemailer": "^6.9.14",
    "ping": "^0.4.4",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.3",
    "tsconfig-paths": "^4.2.0"
  }
}
